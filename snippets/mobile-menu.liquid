    border: 0;
    cursor: pointer;
  }
  .menu--Icon.menu--Icon--close {
    fill: currentColor;
    stroke-width: 1px;
    background: none;
    display: inline-block;
    height: 1em;
    pointer-events: none;
    vertical-align: middle;
    width: 1em;
  }
  ul.mobile--menu-navigation {
    margin: 0;
    padding: 0;
