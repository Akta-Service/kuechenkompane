    pointer-events: none;
    position: fixed;
    right: 0;
    top: 0;
    transform: translate(-100%);
    transition: transform 0.5s;
    z-index: 100;
  }

  .menu--open {
    pointer-events: all;
    transform: translate(0);
  }

  .mobile--menu-header {
    align-items: center;
